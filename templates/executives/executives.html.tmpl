         window.dataLayer = window.dataLayer || [];
         function gtag(){dataLayer.push(arguments);}
         gtag('js', new Date());

         gtag('config', 'UA-77741671-1');
      </script>
      <script>
      function ClearForm(){
      document.executiveForm.reset();
      }
   </script>
