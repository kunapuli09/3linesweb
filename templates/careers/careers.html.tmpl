{{define "layout"}}
<!DOCTYPE html>
<html lang="en">
   <head>
      <meta charset="utf-8">
      <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
      <meta name="description" content="">
      <meta name="author" content="">
      <title>3Lines Venture Capital</title>
      <!-- Bootstrap core CSS -->
      <link href="vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
      <!-- Custom fonts for this template -->
      <link href="vendor/font-awesome/css/all.css" rel="stylesheet" type="text/css">
      <link href="https://fonts.googleapis.com/css?family=Montserrat:400,700" rel="stylesheet" type="text/css" media="print">
      <link href='https://fonts.googleapis.com/css?family=Kaushan+Script' rel='stylesheet' type='text/css' media="print">
      <link href='https://fonts.googleapis.com/css?family=Droid+Serif:400,700,400italic,700italic' rel='stylesheet' type='text/css' media="print">
      <link href='https://fonts.googleapis.com/css?family=Roboto+Slab:400,100,300,700' rel='stylesheet' type='text/css' media="print">
      <!-- date picker end  -->
      <!-- Custom styles for this template -->
      <link href="css/agency.min.css" rel="stylesheet">
      <!-- Favicon.ico -->
      <link rel="apple-touch-icon" sizes="180x180" href="img/favicon/apple-touch-icon.png">
      <link rel="icon" type="image/png" sizes="32x32" href="img/favicon/favicon-32x32.png">
      <link rel="icon" type="image/png" sizes="16x16" href="img/favicon/favicon-16x16.png">
      <link rel="manifest" href="img/favicon/site.webmanifest">
      <link rel="mask-icon" href="img/favicon/safari-pinned-tab.svg" color="#5bbad5">
      <meta name="msapplication-TileColor" content="#00a300">
      <meta name="theme-color" content="#ffffff">
      <script src="https://www.google.com/recaptcha/api.js" async defer></script>
    </script>
   </head>
   <body id="page-top" onload="javascript:ClearForm()">
      <!-- Navigation -->
      <nav class="navbar navbar-expand-lg navbar-dark fixed-top" id="mainNav">
         <div class="container">
            <a class="navbar-brand js-scroll-trigger" href="#page-top"><img src="img/logo.png" alt="3Lines"></a>
            <button class="navbar-toggler navbar-toggler-right" type="button" data-toggle="collapse" data-target="#navbarResponsive" aria-controls="navbarResponsive" aria-expanded="false" aria-label="Toggle navigation">
            Menu
            <i class="fas fa-bars"></i>
            </button>
            <div class="collapse navbar-collapse" id="navbarResponsive">
               <ul class="navbar-nav text-uppercase ml-auto">
                  <!-- <li class="nav-item">
                     <a class="nav-link js-scroll-trigger" href="#services">Services</a>
                     </li> -->
                  <li class="nav-item">
                     <a class="nav-link js-scroll-trigger" href="/#about">About</a>
                  </li>
                  <li class="nav-item">
                     <a class="nav-link js-scroll-trigger" href="/#portfolio">Portfolio</a>
                  </li>
                  <li class="nav-item">
                     <a class="nav-link js-scroll-trigger" href="appl">Apply</a>
                  </li>
                  <li class="nav-item">
                     <a class="nav-link js-scroll-trigger" href="/#team">Team</a>
                  </li>
                  <li class="nav-item">
                     <a class="nav-link js-scroll-trigger" href="/#blog">News & Blogs</a>
                  </li>
                  <li class="nav-item">
                     <a class="nav-link js-scroll-trigger" href="/login">Investors</a>
                  </li>
                  <li class="nav-item">
                     <a class="nav-link js-scroll-trigger" href="/careers">Collaboration</a>
                  </li>
                  <li class="nav-item">
                     <a class="nav-link js-scroll-trigger" href="/#contact">Contact</a>
                  </li>
               </ul>
            </div>
         </div>
      </nav>
         <section id="contact">
            <div class="container">
            <div class="row">
               <div class="col-lg-12 text-center">
                  <h2 class="section-heading">Explore a partnership or a career opportunity with a 3Lines portfolio company<br>
                  </h2>
                  <div class="intro-lead-in text-primary text-center">(Partnership Proposals Or Resumes Only)</div>
               </div>
               
               <div class="intro-lead-in"></div>
               <div class="intro-lead-in"></div>
            </div>
            <div class="row">
            </div>
      <div class="row">
      <div class="col-lg-12">
      <div class="row">
        <div class="col-md-6">
                <p style="color:White;">
               3Lines Venture Capital invests in exceptional founders and startups working on broad disruptive themes around the Future of Work, AI enabled enterprises, and Industry 4.0. Most of the companies are earlystage and would like to explore collaboration partnerships. A good example would be a company looking for a sales channel partner. Many of the fast growing startups also look for highly talented individuals that are seeking jobs in the startup space. This portal doesn't gaurantee a partnership or a job. </p>
               <p style="color:White;">
               We will make the introduction if the fit is right. Please submit less confidential information to start a dialogue.</p>
              
      </div>
      <div class="col-md-6">
       {{if .SuccessMessage }}
      <div class="alert alert-success" id="successSubmit"><strong>{{.SuccessMessage}}</strong>
      <button type="button" class="close" data-dismiss="alert" aria-label="Close">
      <span aria-hidden="true">&times;</span>
      </button>
      </div>
      {{ end }}
      <form id="careersForm" enctype="multipart/form-data" class="form-signup-login" method="post" action="/addProposalDoc">
      <div class="form-group">
        <label for="Startup_ID"><span style="color:White;">Choose a startup to work with</span></label>
        <select class="form-control" id="Investment_ID" name="Investment_ID">
          {{ range .Investments}}
          <option value="{{.ID}}">{{.StartupName}}</option>
          {{end}}
        </select>
      </div>
      <div class="form-group">
      <input class="form-control" id="FullName" name="FullName" type="text" placeholder="Full Name *" required data-validation-required-message="Please enter your full name.">
      <p class="help-block text-danger"></p>
      </div>
      <div class="form-group">
      <input class="form-control" id="Email" name="Email" type="email" placeholder="Your Email *" required data-validation-required-message="Please enter your email address.">
      <p class="help-block text-danger"></p>
      </div>
      <div class="form-group">
      <input class="form-control" id="Phone" name="Phone" type="tel" pattern='^(\d{10}|\d{11}|\d{12})$' placeholder="Phone Maximum 12 Digits.  No + or - format" required data-validation-required-message="Maximum 12 Digits in a Phone Number including Country Code. No (+) or (-) Sign Reqired for International.">
      <p class="help-block text-danger"></p>
      </div>
      <div class="form-group">
      <input class="form-control" id="CompanyName" name="CompanyName" type="text" placeholder="Company Name *" data-validation-required-message="Please enter your company name.">
      <p class="help-block text-danger"></p>
      </div>
      <div class="form-group">
        <label for="Doc"><span style="color:White;">Upload a business proposal or a resume in a PDF Format</span></label>
        <input type="file" class="form-control-file" name= "Doc" id="Doc" style="color:White;">
      </div>
      <!-- <div id="g-recaptcha3" name="rcres" class="g-recaptcha"></div> -->
      <div class="g-recaptcha" data-sitekey="6LcF3OQUAAAAAGmMrHmVIWUp4qxjL8wdLnGR6k-w"></div>
      <button class="btn btn-success btn-xl text-uppercase" type="submit">Submit</button>
      </div>
      </form>
      </div>
      </div>
      </section>
      <!-- Bootstrap core JavaScript -->
      <script src="vendor/jquery/jquery.min.js"></script>
<!--       <script src="vendor/bootstrap/js/popper.min.js"></script> -->
      <script src="vendor/bootstrap/js/bootstrap.bundle.js"></script>
      <!-- Plugin JavaScript -->
      <script src="vendor/jquery-easing/jquery.easing.min.js"></script>
      <!-- Custom scripts for this template -->
      <script src="js/agency.min.js"></script>
      <!-- date picker start  -->
      <!-- Global site tag (gtag.js) - Google Analytics -->
      <script async src="https://www.googletagmanager.com/gtag/js?id=UA-77741671-1"></script>
      <script>
         window.dataLayer = window.dataLayer || [];
         function gtag(){dataLayer.push(arguments);}
         gtag('js', new Date());

         gtag('config', 'UA-77741671-1');
      </script>
      <script>
      function ClearForm(){
      document.careersForm.reset();
      }
   </script>
   </body>
</html>
{{end}}
