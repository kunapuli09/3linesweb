<!-- DATA TABLE-->
<section class="p-t-20">
   <div class="container">
      <div class="row">
         <div class="col-md-12">
            <h3 class="title-5 m-b-35">Investment Portfolio: Click on a Company Name for Details</h3>
            {{if .Assessments}}
            <div class="table-responsive table--no-card m-b-30">
               <table class="table table-hover table-borderless table-striped table-earning ">
                  <thead>
                     <tr>
                        <th>Company Name</th>
                        <th>Playbook Rating</th>
                        <th>Team</th>
                        <th>Revenue Growth</th>
                        <th>Return Multiple Projection</th>
                     </tr>
                  </thead>
                  <tbody>
                     {{range .Assessments }}
                     <tr id="{{.Investment_ID}}" class="tr-shadow">
                        <td>{{.StartupName}}</td>
                        <td>
                           {{ if eq "SlowGrowth" .Status }}
                           <p class="text-danger">Slow Growth
                           {{ end }}
                           {{ if eq "HighPotential" .Status }}
                           <p class="text-info">High Potential</p>
                           {{ end }}
                           {{ if eq "Star" .Status }}
                           <p class="text-success">Star</p>
                           {{ end }}
                        </td>
                        <td>
                           {{ if eq "SlowGrowth" .Status }}
                           <p class="text-danger">B-C
                           {{ end }}
                           {{ if eq "HighPotential" .Status }}
                           <p class="text-info">A-B+</p>
                           {{ end }}
                           {{ if eq "Star" .Status }}
                           <p class="text-success">A+</p>
                           {{ end }}
                        </td>
                        <td>
                           {{ if eq "SlowGrowth" .Status }}
                           <p class="text-danger">Less than 50% of Plan
                           {{ end }}
                           {{ if eq "HighPotential" .Status }}
                           <p class="text-info">50-70% of Plan</p>
                           {{ end }}
                           {{ if eq "Star" .Status }}
                           <p class="text-success">70%+ of Plan</p>
                           {{ end }}
                        </td>
                        <td>
                           {{ if eq "SlowGrowth" .Status }}
                           <p class="text-danger">1-2X
                           {{ end }}
                           {{ if eq "HighPotential" .Status }}
                           <p class="text-info">More than 5X</p>
                           {{ end }}
                           {{ if eq "Star" .Status }}
                           <p class="text-success">More than 10X</p>
                           {{ end }}
                        </td>
                     </tr>
                     <tr class="spacer"></tr>
                     {{end}}
                  </tbody>
               </table>
            </div>
            {{end}}
         </div>
      </div>
   </div>
</section>
<!-- END DATA TABLE-->
{{end}}